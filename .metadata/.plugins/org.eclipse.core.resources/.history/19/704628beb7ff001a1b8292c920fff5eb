package quizModel;

import java.io.Serializable;

public class AnswerData implements Serializable {
	private double[] answer;
	private int score;

	public AnswerData() {    }

	public double[] getAnswer() {
		return answer;
	}

	public void setAnswer(double[] answer) {
		this.answer = answer;
	}

	public int getScore() {
		return score;
	}

	public void setScore(int score) {
		this.score = score;
	}


}
