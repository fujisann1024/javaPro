
public class test {
	public static void main(String[] args) {
		//int型の割り算
		double number = 13.55;
		number = (double)Math.round(number * 10)/10;
		System.out.println(number);
		double number1 = 10.0000;
		double number2 = 3;
		double pro = number1 / number2;
		System.out.println(pro);
		int answer = 10.000;
		if(number1 == answer) {
			System.out.println("true");
		}

	}
}
